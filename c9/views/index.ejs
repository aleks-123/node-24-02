<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Studenska Web Stranica</title>
  </head>
  <body>
    <h1>Ime: <%= ime %> <%= prezime %></h1>
    <ul>
      <% niza.forEach(grad => { %>
      <li><%= grad %></li>
      <% }); %>
    </ul>
    <table border="1" width="100%">
      <tr>
        <th>Ime</th>
        <th>Prezime</th>
        <th>Prosek</th>
      </tr>
      <% studenti.forEach(s => { %>
      <tr>
        <td><%= s.ime %></td>
        <td><%= s.prezime %></td>
        <td><%= s.prosek %></td>
      </tr>
      <% }) %>
    </table>
    <a href="http://localhost:10000/test">Go to test webpage</a>
  </body>
</html>
